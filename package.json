{
  "name": "voice-compass-figma-plugin",
  "version": "0.0.3",
  "description": "Your Figma plugin",
  "main": "code.js",
  "scripts": {
    "lint": "eslint src/**/*.tsx",
    "prebuild": "rimraf lib",
    "build:code": "tsc && rimraf lib/ui.js",
    "build:ui": "webpack --mode=production && rimraf lib/ui.js",
    "build": "npm run build:code && npm run build:ui",
    "dev": "chokidar 'src/*' -c 'npm run build'",
    "package": "npm run build && rm -rf voice-compass-figma-plugin && mkdir -p voice-compass-figma-plugin/lib && cp -r lib voice-compass-figma-plugin && cp manifest.json voice-compass-figma-plugin && cp package.json voice-compass-figma-plugin"
  },
  "author": "Peter Szerzo<peter@nlx.ai>",
  "license": "",
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/plugin-transform-react-jsx": "^7.12.17",
    "@babel/preset-react": "^7.12.13",
    "@babel/preset-typescript": "^7.13.0",
    "@figma/plugin-typings": "^1.49.0",
    "@types/uuid": "^8.3.0",
    "@types/webpack-env": "^1.16.0",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "babel-loader": "^8.2.2",
    "chokidar-cli": "^2.1.0",
    "eslint": "^7.22.0",
    "eslint-config-preact": "^1.1.3",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "html-webpack-inline-source-plugin": "^0.0.10",
    "html-webpack-plugin": "^5.3.1",
    "prettier": "^2.2.1",
    "react-dev-utils": "^12.0.1",
    "rimraf": "^3.0.2",
    "style-loader": "^2.0.0",
    "typescript": "^4.2.3",
    "webpack": "^5.26.2",
    "webpack-cli": "^4.5.0"
  },
  "dependencies": {
    "date-fns": "^2.19.0",
    "fuse.js": "^6.4.6",
    "htm": "^3.0.4",
    "lz-string": "^1.4.4",
    "preact": "^10.5.13",
    "react-icons": "^4.4.0",
    "uuid": "^8.3.2"
  }
}
